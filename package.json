{
  "name": "kasir-be",
  "version": "1.0.0",
  "description": "Backend API untuk aplikasi kasir dengan TypeScript, Prisma, dan PostgreSQL",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && tsc-alias && ts-add-js-extension --dir=dist",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma generate && prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "seed": "tsx src/prisma/seeder.ts",
    "dev": "tsx watch src/index.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "imports": {
    "#*": "./dist/*"
  },
  "dependencies": {
    "@prisma/client": "7.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "7.4.0",
    "ts-add-js-extension": "^1.6.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
